    border-radius: 10px;
    color: rgb(255, 255, 255);
}

/* Styling for the QComboBox when hovered */
QComboBox:hover {
    background-color: rgb(0, 75, 117); /* Slightly darker color when hovered */
}

/* Styling for the QComboBox when the dropdown is activated */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: right center;
    width: 15px;
    border-left-width: 0px;
    border-left-color: darkgray;
    border-left-style: solid; /* just a single line */
    border-top-right-radius: 10px; /* same radius as the QComboBox */
    border-bottom-right-radius: 10px; /* same radius as the QComboBox */
}

